<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.practica3.ui.home.BikeFragment"
        android:label="Available bikes"
        tools:layout="@layout/fragment_bike"

        />

    <fragment
        android:id="@+id/nav_selection"
        android:name="com.example.practica3.FirstFragment"
        android:label="Select Date"
        tools:layout="@layout/fragment_first" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.practica3.ui.slideshow.MapsFragment"
        android:label="Map"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/nav_register"
        android:name="com.example.practica3.MyBikeFragment"
        android:label="Register Bike"
        tools:layout="@layout/fragment_my_bike" />
</navigation>